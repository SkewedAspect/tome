{
  "name": "tome",
  "version": "0.1.0",
  "description": "A simple wiki.",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist/*; rm -rf .cache",
    "copy": "cp -r client/static dist/",
    "build": "npm run clean && npm run copy && parcel build client/index.html",
    "watch": "npm run clean && npm run copy && parcel watch client/index.html",
    "test": "LOG_LEVEL=ERROR UNIT_TESTS=TRUE mocha tests/ --recursive --exit --delay",
    "start": "DEBUG=TRUE node server.js"
  },
  "author": "Christopher S. Case <chris.case@g33xnexus.com>",
  "license": "MIT",
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "dependencies": {
    "bluebird": "^3.4.3",
    "body-parser": "^1.15.2",
    "connect-session-knex": "^1.4.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "knex": "^0.14.4",
    "lodash": "^4.17.5",
    "moment": "^2.19.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-google-web": "^1.0.0",
    "sqlite3": "^4.0.0",
    "trivial-logging": "^1.2.0",
    "trivialperms": "^1.0.0-rc.4"
  },
  "devDependencies": {
    "@fortawesome/fontawesome": "^1.1.3",
    "@fortawesome/fontawesome-free-brands": "^5.0.6",
    "@fortawesome/fontawesome-pro-solid": "^5.0.6",
    "@fortawesome/vue-fontawesome": "0.0.22",
    "@vue/component-compiler-utils": "1.2.1",
    "autoprefixer": "^8.0.0",
    "axios": "^0.18.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "bootstrap": "^4.1.1",
    "bootstrap-vue": "^2.0.0-rc.9",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "codemirror": "^5.38.0",
    "commander": "^2.15.1",
    "highlightjs": "^9.10.0",
    "jquery": "^3.2.1",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^4.0.0",
    "markdown-it-table-of-contents": "^0.3.3",
    "markdown-it-task-lists": "^2.1.0",
    "mocha": "^5.0.1",
    "node-sass": "^4.6.0",
    "parcel": "^1.8.0",
    "precss": "^3.1.1",
    "rxjs": "^6.2.0",
    "vue": "^2.3.3",
    "vue-cm": "^1.1.0",
    "vue-hot-reload-api": "^2.3.0",
    "vue-router": "^3.0.1",
    "vue-rx": "^5.0.0",
    "vue-template-compiler": "^2.5.11"
  }
}
